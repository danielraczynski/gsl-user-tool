                  <xs:sequence>
                    <xs:element minOccurs="0" name="hostname" type="xs:string" />
                    <xs:element minOccurs="0" name="port" type="xs:int" />
                    <xs:element minOccurs="0" name="failoverType" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>