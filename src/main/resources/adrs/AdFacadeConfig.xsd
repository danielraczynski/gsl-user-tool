              <xs:element minOccurs="0" name="loadBalancingEnabled" type="xs:boolean" />
              <xs:element minOccurs="0" default="" name="netbiosName" type="xs:string" />
              <xs:element minOccurs="0" name="exchangeWebServiceUrl" type="xs:string" />
