<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:mstns="http://adrs.gds.roche.com/AdFacadeConfig.xsd" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns="http://adrs.gds.roche.com/AdFacadeConfig.xsd" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://adrs.gds.roche.com/AdFacadeConfig.xsd" id="AdFacadeConfig" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element msdata:IsDataSet="true" msdata:Locale="en-US" name="AdFacadeConfig">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="Domain">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" name="domainName" type="xs:string" />
